# CloudProject — example env file
# Copy to env/.env and replace the CHANGE_ME values.

# Optional: container timezone
TZ=Europe/Rome

# Nextcloud admin (used by Nextcloud and the exporter)
NEXTCLOUD_ADMIN_USER=admin
NEXTCLOUD_ADMIN_PASSWORD=CHANGE_ME_strong_admin_password

# Space-separated hostnames Nextcloud will accept
NEXTCLOUD_TRUSTED_DOMAINS=localhost 127.0.0.1 nextcloud

# Grafana admin
GF_SECURITY_ADMIN_PASSWORD=CHANGE_ME_strong_grafana_password
